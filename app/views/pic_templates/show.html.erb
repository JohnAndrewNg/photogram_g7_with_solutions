<h1>Photo Details</h1>

<img src="<%= @my_photo.source %>">

<p>
  <%= @my_photo.caption %>
</p>

<p>
  <%= time_ago_in_words(@my_photo.created_at) %>
</p>

<a href="/photos" class="btn btn-secondary">Go back</a>
<a href="/photos/<%= @my_photo.id %>/edit" class="btn btn-secondary">Edit</a>
<a href="/delete_photo/<%= @my_photo.id %>" class="btn btn-secondary">Delete</a>
